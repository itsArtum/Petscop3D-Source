if(!global.replaying) {

if(keyboard_check_pressed(vk_tab) &amp;&amp; alarm[0] &lt;= 0) {
    alarm[0] = 60*6;
}

if(alarm[0] &lt;= 10) {
    shown = false;
} else if(alarm[0] &gt;= 20) {
    shown = true;
}

if(controller &gt; 3) {
controller = 0;
}

if(shown = true) {
    if(keyboard_check_pressed(vk_tab) &amp;&amp; h &gt; 0) {
        controller++;
        alarm[0] = 60*6;
    }
